{
  "compilerOptions": {
    "outDir": "./dist/mjs",
    "target": "ES2022",
    "module": "ES2022",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "strict": true,
    "skipLibCheck": true,
    
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "*.ts"]
}
